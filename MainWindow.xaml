			<Grid>
				<Grid.Resources>
					<Style TargetType="{x:Type TextBox}">
						<Setter Property="Width" Value="50"/>
						<Setter Property="Height" Value="50"/>
						<Setter Property="FontSize" Value="25"/>
					</Style>
				</Grid.Resources>
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"/>
					<RowDefinition Height="*"/>
					<RowDefinition Height="*"/>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="*"/>
					<ColumnDefinition Width="Auto"/>
					<ColumnDefinition Width="Auto"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">Tile Group Orientation</TextBlock>
				<TextBlock Grid.Row="1" Grid.Column="0" Grid.RowSpan="2">NESW for rotation no flip<LineBreak/>0369 for rotation with flip</TextBlock>
				<TextBox Grid.Row="1" Grid.Column="1" Text="{Binding TileOrientation00}" />
				<TextBox Grid.Row="1" Grid.Column="2" Text="{Binding TileOrientation01}" />
				<TextBox Grid.Row="2" Grid.Column="1" Text="{Binding TileOrientation10}" />
				<TextBox Grid.Row="2" Grid.Column="2" Text="{Binding TileOrientation11}" />
			</Grid>
			<Grid>
				<Grid.Resources>
					<Style TargetType="{x:Type TextBox}">
						<Setter Property="Width" Value="120"/>
						<Setter Property="Height" Value="30"/>
						<Setter Property="FontSize" Value="25"/>
					</Style>
				</Grid.Resources>
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"/>
					<RowDefinition Height="*"/>
					<RowDefinition Height="*"/>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="*"/>
					<ColumnDefinition Width="Auto"/>
					<ColumnDefinition Width="Auto"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">Color Wiring</TextBlock>
				<TextBox Grid.Row="1" Grid.Column="1" Text="{Binding ColorWiring00}" />
				<TextBox Grid.Row="1" Grid.Column="2" Text="{Binding ColorWiring01}" />
				<TextBox Grid.Row="2" Grid.Column="1" Text="{Binding ColorWiring10}" />
				<TextBox Grid.Row="2" Grid.Column="2" Text="{Binding ColorWiring11}" />
			</Grid>
		</StackPanel>
	</Grid>
