				<TextBox Grid.Row="1" Grid.Column="1" Text="{Binding Cell00}" />
				<TextBox Grid.Row="1" Grid.Column="2" Text="{Binding Cell01}" />
				<TextBox Grid.Row="1" Grid.Column="3" Text="{Binding Cell02}" />
				<TextBox Grid.Row="1" Grid.Column="4" Text="{Binding Cell03}" />
				<TextBox Grid.Row="2" Grid.Column="1" Text="{Binding Cell10}" />
				<TextBox Grid.Row="2" Grid.Column="2" Text="{Binding Cell11}" />
				<TextBox Grid.Row="2" Grid.Column="3" Text="{Binding Cell12}" />
				<TextBox Grid.Row="2" Grid.Column="4" Text="{Binding Cell13}" />
				<TextBox Grid.Row="3" Grid.Column="1" Text="{Binding Cell20}" />
				<TextBox Grid.Row="3" Grid.Column="2" Text="{Binding Cell21}" />
				<TextBox Grid.Row="3" Grid.Column="3" Text="{Binding Cell22}" />
				<TextBox Grid.Row="3" Grid.Column="4" Text="{Binding Cell23}" />
				<TextBox Grid.Row="4" Grid.Column="1" Text="{Binding Cell30}" />
